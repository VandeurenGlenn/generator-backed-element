{
  "bundles": [
    {
      "src": "src/index.js",
      "dest": "app/index.js",
      "format": "cjs",
      "plugins": {
        "babel": {
          "presets": [
            [
              "es2015",
              {
                "modules": false
              }
            ]
          ],
          "plugins": [
            "external-helpers"
          ]
        }
      }
    }, {
      "src": "src/element.js",
      "dest": "element/index.js",
      "format": "cjs",
      "plugins": {
        "babel": {
          "presets": [
            [
              "es2015",
              {
                "modules": false
              }
            ]
          ],
          "plugins": [
            "external-helpers"
          ]
        }
      }
    }
  ]
}
